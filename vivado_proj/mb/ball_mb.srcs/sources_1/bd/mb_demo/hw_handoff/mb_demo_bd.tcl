  # Make sure parentObj is hier blk
  set parentType [get_property TYPE $parentObj]
  if { $parentType ne "hier" } {
     catch {common::send_msg_id "BD_TCL-101" "ERROR" "Parent <$parentObj> has TYPE = <$parentType>. Expected to be <hier>."}
     return
  }

  # Save current instance; Restore later
  set oldCurInst [current_bd_instance .]

  # Set parent object as current
  current_bd_instance $parentObj


  # Create interface ports
  set OV7725_IIC [ create_bd_intf_port -mode Master -vlnv xilinx.com:interface:iic_rtl:1.0 OV7725_IIC ]
  set dip_switches_16bits [ create_bd_intf_port -mode Master -vlnv xilinx.com:interface:gpio_rtl:1.0 dip_switches_16bits ]
  set iic_rtl [ create_bd_intf_port -mode Master -vlnv xilinx.com:interface:iic_rtl:1.0 iic_rtl ]
  set usb_uart [ create_bd_intf_port -mode Master -vlnv xilinx.com:interface:uart_rtl:1.0 usb_uart ]

  # Create ports
  set OV7725_D_0 [ create_bd_port -dir I -from 7 -to 0 OV7725_D_0 ]
  set OV7725_HREF_0 [ create_bd_port -dir I OV7725_HREF_0 ]
  set OV7725_PCLK_0 [ create_bd_port -dir I OV7725_PCLK_0 ]
  set OV7725_PWND_0 [ create_bd_port -dir O OV7725_PWND_0 ]
  set OV7725_RST_0 [ create_bd_port -dir O -type rst OV7725_RST_0 ]
  set OV7725_VSYNC_0 [ create_bd_port -dir I OV7725_VSYNC_0 ]
  set OV7725_XCLK_0 [ create_bd_port -dir O OV7725_XCLK_0 ]
  set en_take_color_i_0 [ create_bd_port -dir I en_take_color_i_0 ]
  set pwm_out_0_0 [ create_bd_port -dir O pwm_out_0_0 ]
  set pwm_out_1_0 [ create_bd_port -dir O pwm_out_1_0 ]
  set reset [ create_bd_port -dir I -type rst reset ]
  set_property -dict [ list \
   CONFIG.POLARITY {ACTIVE_HIGH} \
 ] $reset
  set sys_clock [ create_bd_port -dir I -type clk sys_clock ]
  set_property -dict [ list \
   CONFIG.FREQ_HZ {100000000} \
   CONFIG.PHASE {0.000} \
 ] $sys_clock
  set vga_data_0 [ create_bd_port -dir O -from 11 -to 0 vga_data_0 ]
  set vga_hsync_0 [ create_bd_port -dir O vga_hsync_0 ]
  set vga_vsync_0 [ create_bd_port -dir O vga_vsync_0 ]

  # Create instance: alinx_ov5640_0, and set properties
  set block_name alinx_ov5640
  set block_cell_name alinx_ov5640_0
  if { [catch {set alinx_ov5640_0 [create_bd_cell -type module -reference $block_name $block_cell_name] } errmsg] } {
     catch {common::send_msg_id "BD_TCL-105" "ERROR" "Unable to add referenced block <$block_name>. Please add the files for ${block_name}'s definition into the project."}
     return 1
   } elseif { $alinx_ov5640_0 eq "" } {
     catch {common::send_msg_id "BD_TCL-106" "ERROR" "Unable to referenced block <$block_name>. Please add the files for ${block_name}'s definition into the project."}
     return 1
   }
  
  # Create instance: axi_gpio_1, and set properties
  set axi_gpio_1 [ create_bd_cell -type ip -vlnv xilinx.com:ip:axi_gpio:2.0 axi_gpio_1 ]
  set_property -dict [ list \
   CONFIG.C_ALL_INPUTS {1} \
   CONFIG.C_ALL_INPUTS_2 {1} \
